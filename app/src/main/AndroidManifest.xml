<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.retical.virtual">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/edulogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/edulogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StudentLogin"></activity>
        <activity android:name=".Profile" />
        <activity android:name=".MessageAdapter" />
        <activity android:name=".Chat" />
        <activity android:name=".CreateRoom" />
        <activity android:name=".JoinRoom" />
        <activity android:name=".Home" />
        <activity android:name=".RegisterStudent" />
        <activity android:name=".Room" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".TeacherLogin" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>